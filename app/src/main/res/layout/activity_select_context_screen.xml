<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/selectedcontext"
    tools:context="com.example.Screen.SelectContextScreen">

    <Button
        android:id="@+id/education_button"
        android:layout_width="280dp"
        android:layout_height="43dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="152dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/adventure_button"
        android:layout_width="215dp"
        android:layout_height="47dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="152dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/education_button"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/comedy_button"
        android:layout_width="157dp"
        android:layout_height="39dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="48dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/education_button" />

    <Button
        android:id="@+id/science_button"
        android:layout_width="269dp"
        android:layout_height="48dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="288dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/comedy_button"
        app:layout_constraintTop_toBottomOf="@+id/adventure_button" />

    <Button
        android:id="@+id/food_button"
        android:layout_width="219dp"
        android:layout_height="40dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="48dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/health_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comedy_button"
        app:layout_constraintVertical_bias="0.17" />

    <Button
        android:id="@+id/knowledge_button"
        android:layout_width="252dp"
        android:layout_height="47dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="228dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/book_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/food_button"
        app:layout_constraintTop_toBottomOf="@+id/science_button"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/health_button"
        android:layout_width="253dp"
        android:layout_height="48dp"
        android:layout_marginEnd="96dp"
        android:layout_marginBottom="52dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/book_button"
        app:layout_constraintHorizontal_bias="0.987"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/book_button"
        android:layout_width="133dp"
        android:layout_height="48dp"
        android:layout_marginEnd="316dp"
        android:layout_marginBottom="44dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/start__chat_button"
        android:layout_width="153dp"
        android:layout_height="35dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="42dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/book_button"
        app:layout_constraintVertical_bias="0.88" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:layout_marginStart="284dp"
        android:layout_marginBottom="44dp"
        android:fontFamily="@font/poppins_semibold"
        android:text=""
        android:textColor="#D5CECE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>