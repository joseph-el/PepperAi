<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/about2"
    tools:context="com.example.Screen.AboutScreen.AboutScreen_2">

    <Button
        android:id="@+id/left_button_about_2"
        android:layout_width="0dp"
        android:layout_height="46dp"
        android:layout_marginStart="638dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="36dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/next_button_about_2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/next_button_about_2"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginEnd="175dp"
        android:layout_marginBottom="38dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/left_button_about_2" />
</androidx.constraintlayout.widget.ConstraintLayout>